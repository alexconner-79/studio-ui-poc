{
  "name": "studio-poc",
  "private": true,
  "scripts": {
    "build:compiler": "tsc -p tsconfig.compiler.json",
    "dev": "ts-node compiler/dev.ts",
    "compile": "node dist/compiler/compile.js || ts-node compiler/compile.ts",
    "compile:ts": "ts-node compiler/compile.ts",
    "watch": "ts-node compiler/watch.ts",
    "studio": "ts-node compiler/scaffold.ts",
    "lint:specs": "ts-node compiler/lint.ts",
    "publish:dry": "ts-node compiler/publish.ts --dry",
    "publish:commit": "ts-node compiler/publish.ts --commit",
    "publish:pr": "ts-node compiler/publish.ts --pr"
  },
  "devDependencies": {
    "@octokit/rest": "^22.0.1",
    "@types/node": "^25.2.2",
    "ajv": "latest",
    "ajv-formats": "latest",
    "chalk": "^5.6.2",
    "chokidar": "latest",
    "prettier": "latest",
    "simple-git": "latest",
    "ts-node": "^10.9.2",
    "tsx": "latest",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "@types/archiver": "^7.0.0",
    "@types/babel__traverse": "^7.28.0",
    "archiver": "^7.0.1",
    "jszip": "^3.10.1"
  }
}
